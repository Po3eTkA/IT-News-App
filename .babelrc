{
  "presets": [["env", {
    "targets": {
      "browsers": ["last 2 versions", "ie >= 11"]
    }
  }],
  "react"],
  "env": {
    "loose": true,
    "modules": false,
    "production": {
      "presets": ["react-optimize"]
    }
  },
  "plugins": [
    "dynamic-import-webpack",
    "react-hot-loader/babel",
    "styled-components",
    "syntax-dynamic-import",
    "transform-react-jsx-source",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}